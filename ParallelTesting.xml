<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="register Suite" parallel="tests" thread-count="2" >

  <test name="chrome Register Test" >
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="tests.UserParallelTest"/>
    </classes>
 </test>
 
 <test name="firefox Register Test" >
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="tests.UserParallelTest"/>
    </classes>
 </test>
 
 </suite>
